@model HomeIndexViewModel
@{
    ViewBag.Title = "Index";
}

<h1>Index</h1>
<html>
<head>
    <title></title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>ID</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model.Users)
            {
                <tr>
                    <td>
                        @user.Id
                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
